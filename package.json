{
  "name": "cursor-memo-plugin",
  "displayName": "PromptHub",
  "description": "Plugin to save and reuse Cursor chat commands",
  "version": "0.3.3",
  "publisher": "Boruto",
  "icon": "images/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/BoLiDev/cursor-memo-command.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cursor-memo.savePrompt",
        "title": "Save Current Command",
        "category": "Cursor Memo",
        "icon": "$(add)"
      },
      {
        "command": "cursor-memo.removePrompt",
        "title": "Delete",
        "category": "Cursor Memo"
      },
      {
        "command": "cursor-memo.pasteToEditor",
        "title": "Paste to Cursor Chatbox",
        "category": "Cursor Memo"
      },
      {
        "command": "cursor-memo.renamePrompt",
        "title": "Rename",
        "category": "Cursor Memo"
      },
      {
        "command": "cursor-memo.editPrompt",
        "title": "Edit",
        "category": "Cursor Memo"
      },
      {
        "command": "cursor-memo.addCategory",
        "title": "Add Category",
        "category": "Cursor Memo",
        "icon": "$(add)"
      },
      {
        "command": "cursor-memo.renameCategory",
        "title": "Rename",
        "category": "Cursor Memo"
      },
      {
        "command": "cursor-memo.moveToCategory",
        "title": "Move to Category",
        "category": "Cursor Memo"
      },
      {
        "command": "cursor-memo.deleteCategory",
        "title": "Delete",
        "category": "Cursor Memo"
      },
      {
        "command": "cursor-memo.removeCloudCategory",
        "title": "Remove from Local",
        "category": "Cursor Memo"
      },
      {
        "command": "cursor-memo.addPromptToCategory",
        "title": "Add Command to this Category",
        "category": "Cursor Memo",
        "icon": "$(add)"
      },
      {
        "command": "cursor-memo.exportPrompts",
        "title": "Export Commands",
        "category": "Cursor Memo",
        "icon": "$(arrow-up)"
      },
      {
        "command": "cursor-memo.importPrompts",
        "title": "Import Commands",
        "category": "Cursor Memo",
        "icon": "$(arrow-down)"
      },
      {
        "command": "cursor-memo.syncFromGitLab",
        "title": "Sync from GitLab",
        "category": "Cursor Memo",
        "icon": "$(sync)"
      },
      {
        "command": "cursor-memo.manageGitLabToken",
        "title": "Manage GitLab Token",
        "category": "Cursor Memo",
        "icon": "$(key)"
      },
      {
        "command": "cursor-memo.pushToGitLab",
        "title": "Push to GitLab",
        "category": "Cursor Memo",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "cursor-memo.clearLocalStorage",
        "title": "Clear Local Storage",
        "category": "Cursor Memo",
        "icon": "$(clear-all)"
      },
      {
        "command": "cursor-memo.clearCloudStorage",
        "title": "Clear Cloud Storage",
        "category": "Cursor Memo",
        "icon": "$(clear-all)"
      },
      {
        "command": "cursor-memo.searchAllPrompts",
        "title": "Search All Prompts",
        "category": "CursorMemo",
        "icon": "$(search)"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "promptHubPanel",
          "name": "PromptHub"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "cursor-memo.addCategory",
          "when": "view == promptHubPanel && viewItem == localGroup",
          "group": "inline@2"
        },
        {
          "command": "cursor-memo.importPrompts",
          "when": "view == promptHubPanel && viewItem == localGroup",
          "group": "inline@3"
        },
        {
          "command": "cursor-memo.exportPrompts",
          "when": "view == promptHubPanel && viewItem == localGroup",
          "group": "inline@4"
        },
        {
          "command": "cursor-memo.pushToGitLab",
          "when": "view == promptHubPanel && viewItem == localGroup",
          "group": "inline@5"
        },
        {
          "command": "cursor-memo.syncFromGitLab",
          "when": "view == promptHubPanel && viewItem == cloudGroup",
          "group": "inline@1"
        },
        {
          "command": "cursor-memo.manageGitLabToken",
          "when": "view == promptHubPanel && viewItem == cloudGroup",
          "group": "inline@2"
        },
        {
          "command": "cursor-memo.renamePrompt",
          "when": "view == promptHubPanel && viewItem == memoItem",
          "group": "1_modification@1"
        },
        {
          "command": "cursor-memo.editPrompt",
          "when": "view == promptHubPanel && viewItem == memoItem",
          "group": "1_modification@2"
        },
        {
          "command": "cursor-memo.removePrompt",
          "when": "view == promptHubPanel && viewItem == memoItem",
          "group": "1_modification@3"
        },
        {
          "command": "cursor-memo.moveToCategory",
          "when": "view == promptHubPanel && viewItem == memoItem",
          "group": "2_organize"
        },
        {
          "command": "cursor-memo.renameCategory",
          "when": "view == promptHubPanel && viewItem == category",
          "group": "1_modification@1"
        },
        {
          "command": "cursor-memo.deleteCategory",
          "when": "view == promptHubPanel && viewItem == category",
          "group": "1_modification@2"
        },
        {
          "command": "cursor-memo.removeCloudCategory",
          "when": "view == promptHubPanel && viewItem == cloudCategory",
          "group": "1_modification@1"
        },
        {
          "command": "cursor-memo.addPromptToCategory",
          "when": "view == promptHubPanel && viewItem == category",
          "group": "inline"
        }
      ],
      "view/title": [
        {
          "command": "cursor-memo.searchAllPrompts",
          "when": "view == promptHubPanel",
          "group": "navigation@2",
          "icon": "$(search)"
        }
      ],
      "commandPalette": [
        {
          "command": "cursor-memo.savePrompt",
          "when": "true"
        },
        {
          "command": "cursor-memo.removePrompt",
          "when": "true"
        },
        {
          "command": "cursor-memo.pasteToEditor",
          "when": "true"
        },
        {
          "command": "cursor-memo.renamePrompt",
          "when": "true"
        },
        {
          "command": "cursor-memo.editPrompt",
          "when": "true"
        },
        {
          "command": "cursor-memo.addCategory",
          "when": "true"
        },
        {
          "command": "cursor-memo.renameCategory",
          "when": "true"
        },
        {
          "command": "cursor-memo.moveToCategory",
          "when": "true"
        },
        {
          "command": "cursor-memo.deleteCategory",
          "when": "true"
        },
        {
          "command": "cursor-memo.removeCloudCategory",
          "when": "true"
        },
        {
          "command": "cursor-memo.addPromptToCategory",
          "when": "true"
        },
        {
          "command": "cursor-memo.exportPrompts",
          "when": "true"
        },
        {
          "command": "cursor-memo.importPrompts",
          "when": "true"
        },
        {
          "command": "cursor-memo.syncFromGitLab",
          "when": "true"
        },
        {
          "command": "cursor-memo.manageGitLabToken",
          "when": "true"
        },
        {
          "command": "cursor-memo.pushToGitLab",
          "when": "true"
        },
        {
          "command": "cursor-memo.clearLocalStorage",
          "when": "true"
        },
        {
          "command": "cursor-memo.clearCloudStorage",
          "when": "true"
        },
        {
          "command": "cursor-memo.searchAllPrompts",
          "when": "true"
        }
      ],
      "editor/title": [
        {
          "command": "cursor-memo.searchAllPrompts",
          "group": "navigation@1",
          "when": "resourceLangId && resourceLangId != 'Log'"
        }
      ]
    },
    "configuration": {
      "title": "Cursor Memo",
      "properties": {
        "cursorMemo.gitlabDomain": {
          "type": "string",
          "default": "https://gitlab.okg.com",
          "description": "GitLab domain URL"
        },
        "cursorMemo.gitlabProjectId": {
          "type": "string",
          "default": "9993",
          "description": "GitLab project ID for team commands"
        },
        "cursorMemo.gitlabFilePath": {
          "type": "string",
          "default": "prompt.json",
          "description": "Path to the team commands file in the GitLab repository"
        },
        "cursorMemo.gitlabBranch": {
          "type": "string",
          "default": "master",
          "description": "GitLab branch name containing the team commands file"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "lint": "eslint src --ext ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "package": "npx vsce package"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.4.0",
    "glob": "^7.1.7",
    "mocha": "^9.0.2",
    "prettier": "^3.5.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@types/node-fetch": "^2.6.2",
    "node-fetch": "^2.6.7",
    "zod": "^3.22.4"
  }
}
